    <div class="pt-3"></div>


    <div class="pb-3"></div>

    <div class="container direction-rtl" style="display: none;">
        <div class="card mb-3">
          <div class="card-body">
            <div class="row g-3">
              <div class="col-4">
                <div class="feature-card mx-auto text-center">
                  <div class="card mx-auto bg-gray">
                    <p class="mb-0"><b>Vốn thực tế</b></p>
                  </div>
                  <p class="mb-0">10.000.000đ</p>
                </div>
              </div>
  
